{% extends "@View/layout.html.twig" %}

{% block content %}
    <span id="page_label" data-label="registration_page" hidden></span>

    <form id="register_form" name="register_form">
        <input name="nickname" placeholder="Nickname" required><br><br>
        <input name="first_name" placeholder="First name" required><br><br>
        <input name="last_name"  placeholder="Last name" required><br><br>
        <input name="age"  placeholder="Age" required><br><br>
        <input name="password"  placeholder="Password" required type="password"><br><br>
        <input type="button" class="btn btn-primary event_label" name="submit" value="Submit" onclick="registerProcess()">
    </form>
{% endblock %}